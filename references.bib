@mastersthesis{gisele-dissert2019,
author           = {MATOS, Gisele Gomes},
title            = {A cor nas abordagens policiais no Estado de Goiás},
abstract         = "A presente dissertação de mestrado aborda o trato da polícia militar de Goiás para com a questão racial na formação de praças e nas abordagens policiais em uma sociedade racialmente desigual. Para tanto, a investigação ampara-se, no primeiro momento, em termos teóricos e  por meio do método dialético-argumentativo, na própria história do Brasil, na Criminologia, na Sociologia e no constitucionalismo contemporâneo, esse sob a perspectiva do Estado Democrático de Direito, todos ligados ao processo de escravidão do País e à formação do mito da democracia racial. A revisão da literatura e a análise crítica do discurso demonstram que desde as Ordenações do Reino até o período que se seguiu à abolição da escravatura e à Proclamação da República, sob forte influência do positivismo criminológico, o Brasil contou com uma programação criminalizante voltada ao segmento negro, que reflete a dificuldade de implantação da igualdade material nos dias atuais. Nesse cenário, tem relevância o papel da polícia ostensiva e sua atuação no controle social e, por isso, em relação à PMGO, aborda-se a sua formação desde quando o Brasil era dividido em capitanias, com destaque para a herança do positivismo criminológico, e explora-se a matriz curricular do curso de formação do policiamento ostensivo e a sua adequação às orientações da Senasp, a fim de averiguar se há abordagem da questão racial no conteúdo programático das disciplinas. Do POP, extraem-se elementos acerca da terminologia atitude suspeita, na busca de especificidades da abordagem policial que expliquem a escolha de quem será abordado nas atividades rotineiras de patrulhamento. A pesquisa empírica, por sua vez, utilizando o método hipotético-dedutivo, tem por objetivo aferir em que medida a cor da pele constitui fator de suspeição para a escolha de quem será alvo da abordagem policial por estar em atitude suspeita. A análise quantitativa dá-se através dos dados registrados na PSI da SSP/GO, com registros quanto à cor/raça das pessoas abordadas no estado de Goiás no recorte temporal delimitado de abril de 2016 a abril de 2018. Como resultado, tem-se a negligência por parte dos policiais militares no preenchimento da variável cor/raça nas abordagens policiais, que é de registro não obrigatório. Do que se tem registrado, a PMGO aborda mais pretos e pardos. Comparando-se o percentual de brancos, pretos e pardos na população e nas abordagens policiais, não se registra descompasso, mas observa-se que o próprio IBGE, na PNAD contínua, aponta maior coeficiente de variação no percentual da população preta, o que compromete a análise mais acurada. A realização do estudo qualitativo através da aplicação de questionário por survey resta prejudicada em razão da negativa injustificada da PMGO em anuir com a realização da pesquisa.",
school           = {Universidade Federal de Goiás},
address          = {Goiânia},
% pages          = {392},
year             = {2019},
type             = {Dissertação de Mestrado em Direito e Políticas Públicas, PPGDP-UFG, Linha 2. Projeto 2.1},
annote           = "Orientação Prof. Dr. Pedro Sérgio dos Santos"
}

@phdthesis{gisele-tese2024,
author           = {MATOS, Gisele Gomes},
title            = {LGPD e arranjos institucionais de identificação criminal: os controladores dos dados under skin dos acusados e dos condenados no Brasil (abril de 2020 a março de 2024)},
abstract         = "Aborda-se o microssistema legal brasileiro da “proteção” dos dados pessoais e a sua intersecção com o controle penal da criminalidade a partir do regime legal de identificação criminal de acusados e de condenados estabelecido desde a Lei n.º 12.654/2012, apresentando contribuições teóricas e empírico-metodológicas. A investigação ampara-se, em termos teóricos e por meio do método dialético-argumentativo, na revisão da literatura e na operacionalização de conceitos necessários para a resolução dogmática da situação-problema, que envolve dispositivos normativos (art. 5º da Lei de Identificação Criminal e art. 9º-A da Lei de Execução Penal) e uma limitação a direitos e garantias fundamentais, como a privacidade, bem como a princípios que se relacionam com a prova penal, como o devido processo legal; e aos limites da identificação criminal genética do civilmente identificado e seus impactos na solução de autoria de determinados crimes (Res. nº 16/2022/RIBPG). Para os testes das hipóteses (formuladas aos pares sobre essas limitações e dialeticamente testadas) recorre-se à Teoria dos Direitos Fundamentais como referencial teórico. Estruturam-se na Seção 2 aspectos da Lei Geral de Proteção de Dados (LGPD) e do reconhecimento da proteção de dados pessoais como direito fundamental pelo Supremo Tribunal Federal (STF) para, na Seção 3, tratar da integração da lacuna de uma LGPD penal no Brasil por colisão de princípios, traçando-se possível solução integrativa para a resolução de casos concretos. Ao regime legal da identificação criminal dedica-se a Seção 4, atingindo-se o objetivo de parametrizar seus aspectos diferenciais da coleta de material biológico por determinação judicial para fins de identificação criminal e a dos condenados por determinados crimes. Com recurso à análise documental como técnica de pesquisa empírica qualitativa, na Seção 5 explora-se três cases submetidos ao crivo do STF: RE 973.837, ADI 5.545 e Inq 4.979 e 4.923. Como estratégia metodológica quantitativa, usa-se análise estatística descritiva e inferencial, analisam-se dados secundários retrospectivos do Ministério da Justiça e Segurança Pública (MJSP), séries temporais cumulativas do Banco Nacional de Perfis Genéticos (BNPG) (abr.2020-mar.2024), tendo por objeto a coleta do material biológico e o armazenamento do perfil genético dos investigados e dos condenados no BNPG. Como resultado, a análise qualitativa evidenciou uma movimentação institucional diante do silêncio do STF no julgamento do RE 973.837 e a ausência de uma solução que reduzisse a incerteza jurídica tem levado outros tribunais do país a julgamentos díspares. Da análise estatística da Seção 6 verificou-se o crescimento anômalo de identificados criminalmente via determinação judicial, que coaduna com a análise qualitativa dos Inq 4.879 e 4.923, donde extraiu-se determinações do STF de identificação civil e criminal genética dos custodiados em razão do 08/01/2023. Através dos testes estatísticos McFadden e Bonferroni foi possível observar que a Polícia Federal é maior responsável pela coleta do DNA de identificados criminalmente por determinação judicial e pela anomalia na série temporal. Verificou-se maior investimento na inserção de condenados no BNPG, especialmente se comparado com o quantitativo de vestígios, na contramão da expectativa do aporte teórico de uma correlação negativa entre match rate e a taxa de ocorrência de homicídios e de estupros (Doleac, 2016).",
school           = {Centro Universitário de Brasília},
address          = {Brasília, DF},
% pages          = {470},
year             = {2024},
type             = {Tese de Doutorado defendida no Programa de Doutorado em Direito, PPGD-UniCeub, área de concentração em Políticas Públicas, Processo Civil, Processo Penal e Controle Penal. Linha de pesquisa: Políticas Públicas, Processo Civil, Processo e Controle Penal},
annote           = "Orientação Prof. Dr. Antonio Henrique Graciano Suxberger; Co-orientação: prof. Dr. Cleuler Barbosa das Neves"
}

